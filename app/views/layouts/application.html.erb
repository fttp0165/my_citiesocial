<!DOCTYPE html>
<html>
  <head>
    <title>MyCitiesocial</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="turbolinks-cache-control" content="no-chche">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%=render 'shared/frontend/flash' if notice%>
    <%=render 'shared/frontend/navbar'%>
    <%=render 'shared/frontend/top_manu',categories: @categories%>
    
    <%= yield %>
    <%=render 'shared/frontend/footer'%>
    
  </body>
</html>
